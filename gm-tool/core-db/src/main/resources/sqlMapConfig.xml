<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="logImpl" value="LOG4J" />
		<setting name="cacheEnabled" value="true"/>
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="100" />
	</settings>

	<typeAliases>
        <typeAlias alias="UserInfo" type="com.innogrid.gedge.core.model.UserInfo"/>
        <typeAlias alias="HistoryInfo" type="com.innogrid.gedge.core.model.HistoryInfo"/>
        <typeAlias alias="CredentialInfo" type="com.innogrid.gedge.core.model.CredentialInfo"/>
        <typeAlias alias="GroupInfo" type="com.innogrid.gedge.core.model.GroupInfo"/>
        <typeAlias alias="RoleInfo" type="com.innogrid.gedge.core.model.RoleInfo"/>
        <typeAlias alias="ProjectInfo" type="com.innogrid.gedge.core.model.ProjectInfo"/>
        <typeAlias alias="PermissionInfo" type="com.innogrid.gedge.core.model.PermissionInfo"/>
        <typeAlias alias="ServiceDashboardInfo" type="com.innogrid.gedge.core.model.ServiceDashboardInfo"/>
        <typeAlias alias="MeterServerInfo" type="com.innogrid.gedge.core.model.MeterServerInfo"/>
        <typeAlias alias="MeterServerAccumulateInfo" type="com.innogrid.gedge.core.model.MeterServerAccumulateInfo"/>
        <typeAlias alias="ImageInfo" type="com.innogrid.gedge.core.model.ImageInfo"/>
        <typeAlias alias="ImageDetailInfo" type="com.innogrid.gedge.core.model.ImageDetailInfo"/>
        <typeAlias alias="ActionInfo" type="com.innogrid.gedge.core.model.ActionInfo"/>
	</typeAliases>
    
</configuration>
